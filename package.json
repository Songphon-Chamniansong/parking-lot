    "mongoose": "^5.13.5",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "copy-webpack-plugin": "^9.0.1",
    "jest": "^27.5.1",
    "mongodb-memory-server-core": "^7.3.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.5",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
